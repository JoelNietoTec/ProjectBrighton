<h4>Tipos de Cliente</h4>
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#newClientTypeModal">
    Nuevo
</button>
<div ng-controller="clienttypesController as clientTypes">
    <table class="table table-striped table-condensed">
        <thead>
            <th></th>
            <th>#</th>
            <th>Nombre</th>
            <th>Creado</th>
            <th>Modificado</th>
        </thead>
        <tbody>
            <tr ng-repeat="item in items">
                <td class="centered"><a href="#" ng-click="editClientType(item)" data-toggle="modal" data-target="#editClientTypeModal"><i class="fa fa-pencil"></i></a></td>
                <td>{{item.Id}}</td>
                <td>{{item.Name}}</td>
                <td>{{item.relCreateDate}}</td>
                <td>{{item.relModifyDate}}</td>
            </tr>
        </tbody>
    </table>
    @Html.Partial("~/Views/ClientTypes/New.cshtml")
</div>